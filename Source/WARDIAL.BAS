10 PRINT "**********************"
20 PRINT "* INITECH WAR DIALER *"
30 PRINT "**********************"

200 REM DEFAULTS
210 SL$ = "2": REM SLOT
220 SL = VAL(SL$): REM numeric slot for PR#/IN#
230 IF SL < 1 OR SL > 7 THEN PRINT "BAD SLOT": END
240 PR$ = "N": REM PROMOTHEUS
250 EX$ = "N": REM EXTENDER
260 EA$ = "":LD$ = "":D1 = 5:D2 = 2
270 WA$ = "": REM WAR-AREA CODE
280 SN$ = "": REM STARTING NUMBER
290 ES$ = "9999": REM ENDING SUFFIX
300 D% = 1
310 I$ = "1":INC = 1:BL$ = "20"
320 DL$ = "10":TS$ = "60":RD$ = "3": REM DELAY B/F DISCONNECT, TOUCH-TONE SPEED, # OF REDIALS
330 SP$ = "Y": REM DEFAULT SPEAKER STATUS

1000 REM INITIALIZE MODEM
1010 PRINT "INIT MODEM..."
1020 PRINT "SLOT: "; SL$
1030 PR# SL: SPEED= 100
1040 PRINT "ATZ": GOSUB 2000
1050 REM NOCEHO
1060 PRINT "ATE0"
1070 GOSUB 2000
1080 REM NUMERIC RESULT CODES
1090 IN# SL
1100 PRINT "ATV";2 *(PR$ = "Y")
1110 GET A$
1120 GOSUB 2000
1130 REM NO ANSWER
1140 PRINT "ATS0=0": GET A$: GOSUB 2000
1150 REM DELAY BEFORE HANGUP
1160 PRINT "ATS7=";DL$
1170 GET A$
1180 GOSUB 2000
1190 REM DIALING SPEED
1200 PRINT "ATS11=";TS$
1210 GET A$
1220 GOSUB 2000
1230 REM RESPONSE TIME, CARRIER DETECT
1240 PRINT "ATS9=4"
1250 GET A$
1260 GOSUB 2000
1270 REM COMMA DELAY LENGTH
1280 PRINT "ATS8=";D2: GET A$: GOSUB 2000
1290 REM ESCAPE CODE DEAD TIME
1300 PRINT "ATS12=25": GET A$: GOSUB 2000
1310 REM SPEAKER ON/OFF
1320 PRINT "ATM";(SP$ = "Y"): GET A$: GOSUB 2000
1330 END

2000 REM DELAY
2010 FOR P = 1 TO 1000: NEXT
2020 RETURN