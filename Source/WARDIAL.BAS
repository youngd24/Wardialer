10 PRINT "--------------------"
20 PRINT " INITECH WAR DIALER "
30 PRINT "--------------------"

200 REM DEFAULTS
210 SL$ = "2": REM SLOT
220 SL = VAL(SL$): REM numeric slot for PR#/IN#
230 IF SL < 1 OR SL > 7 THEN PRINT "BAD SLOT": END
240 WA$ = "": REM WAR-AREA CODE
250 SN$ = "": REM STARTING NUMBER
260 ES$ = "9999": REM ENDING SUFFIX
270 D% = 1
280 I$ = "1"
290 INC = 1
300 BL$ = "20"
310 DL$ = "10": REM DELAY B/F DISCONNECT
320 TS$ = "60": REM TOUCH-TONE SPEED
330 RD$ = "3": REM # OF REDIALS
340 SP$ = "Y": REM DEFAULT SPEAKER STATUS

350 INPUT "STARTING NUMBER: "; SN$
360 INPUT "ENDING NUMBER: "; EN$
380 PRINT ""

1000 REM *********************************************************************
1010 REM INITIALIZE MODEM
1020 REM *********************************************************************
1030 PRINT "** MODEM INIT **"
1040 PRINT "SLOT: "; SL$
1050 PR# SL: SPEED= 100
1060 PRINT "ATZ": GOSUB 2000
1070 REM NO ECHO
1080 PRINT "ATE0": GOSUB 2000
1090 IN# SL: REM START INPUT FROM MODEM SLOT
1100 REM NUMERIC RESULT CODES
1110 PRINT "ATV0"
1120 GET A$: GOSUB 2000
1130 REM SHOW NUMERIC RESULT CODES
1140 PRINT "ATQ0"
1150 GET A$: GOSUB 2000
1160 REM ENABLE ALL RESULT CODES
1170 PRINT "ATX4"
1180 GET A$: GOSUB 2000
1190 REM NO ANSWER
1200 PRINT "ATS0=0": GET A$: GOSUB 2000
1210 REM DELAY BEFORE HANGUP
1220 PRINT "ATS7=";DL$
1230 GET A$: GOSUB 2000
1240 REM DIALING SPEED
1250 PRINT "ATS11=";TS$
1260 GET A$: GOSUB 2000
1270 REM RESPONSE TIME, CARRIER DETECT
1280 PRINT "ATS9=4"
1290 GET A$: GOSUB 2000
1300 REM COMMA DELAY LENGTH
1310 PRINT "ATS8=";D2
1320 GET A$: GOSUB 2000
1330 REM ESCAPE CODE DEAD TIME
1340 PRINT "ATS12=25"
1350 GET A$: GOSUB 2000
1360 REM SPEAKER ON/OFF
1370 PRINT "ATM";(SP$ = "Y")
1380 GET A$: GOSUB 2000

1500 END

2000 REM DELAY
2010 FOR P = 1 TO 1000: NEXT
2020 RETURN
